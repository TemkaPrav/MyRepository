<script>
function startChatWith(uname, nick) {
    // Using a try/catch override IE issues
    try {
      var t = 'chat';
      if(uname.match(/@conference/)) {
        t = 'groupchat';
      }
      chatMini(t, uname, nick || uname , hex_md5(uname) );
    }
    
    catch(e) {}
    
    finally {
      return false;
    }
}    
