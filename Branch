<script type="text/javascript">
function text_if_defined(text, def){
    if (text == '') return def;
    return text;
}
function Show_or_Hide_Subling(button){
  button = $(button).parent();
  var object = button.find('.cut').first();
  var top_button = button.find('.wrapper_top').first();
  var bottom_button = button.find('.wrapper_bottom').first();
  if (object.css('display') == 'none' || object.css('visibility') == 'hidden' || object.css('overflow') == 'hidden'){
    top_button.find('img').first().attr('src','/textbooks/artwork/img/less.png');
    $(top_button.find('span')[0]).hide();
    $(top_button.find('span')[1]).show();
    bottom_button.show();
    object.show();
  } else {
    top_button.find('img').first().attr('src','/textbooks/artwork/img/more.png');
    $(top_button.find('span')[1]).hide();
    $(top_button.find('span')[0]).show();
    bottom_button.hide();
    object.hide();
    window.scroll(0,top_button.adjacent('img')[0].offsetTop);
  }
  return false;
}


function addCutWrapping() {
  var cut_close_button = 
    "<div class='wrapper_bottom' onClick='Show_or_Hide_Subling(this)' style='display:none'><img src='/textbooks/artwork/img/direction_up.png' title='згорнути' alt='згорнути'></div>";
  $('div.cut').each(function(){
    var obj = $(this);
    var more = text_if_defined(obj.find('span.if_hide').text(), 'Детальніше');
    var less = text_if_defined(obj.find('span.if_show').text(), 'Згорнути');
    var show_hide_button =
      '<div class="wrapper_top" onClick="Show_or_Hide_Subling(this);">'+
        ' <img src="/textbooks/artwork/img/more.png" title="Відкрити|Сховати"></img>'+
        ' <span>'+more+'</span><span style="display:none">'+less+'</span></div>';
    obj.wrap("<div class='wrapper'></div>");
    obj.before(show_hide_button).after(cut_close_button).hide();
  });
}
addCutWrapping();
</script>



